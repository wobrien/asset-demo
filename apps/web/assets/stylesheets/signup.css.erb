header {
  position: fixed;
  z-index: 2000000;
  top: 0px;
  left: 0px;
  height: 80px;
  width: 100%;
  background-color: white;
  border-top: 8px solid #faa900;
  border-bottom: 1px solid #efefef; 
}
header .logo, header a.logo {
  display: block;
  background-size: 200px 45px;
  width: 200px;
  height: 45px;
  margin: 18px 0px 0px 0px;
  text-indent: -9999px;
  background-image: url("<%= asset_path('tccslogo.png') %>");
  background-repeat: no-repeat; 
}
    





